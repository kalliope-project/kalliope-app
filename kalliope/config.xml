<?xml version='1.0' encoding='utf-8'?>
<widget id="kalliope.project" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Kalliope</name>
    <description>An awesome Ionic/Cordova app For The Kalliope Project</description>
    <author email="kalliope.project@gmail.com" href="https://kalliope-project.github.io/">Kalliope Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://ionic.local/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/kalliope_brain/drawable-xxxhdpi-kalliope_brain.png" />
        <icon density="mdpi" src="resources/android/kalliope_brain/drawable-xxxhdpi-kalliope_brain.png" />
        <icon density="hdpi" src="resources/android/kalliope_brain/drawable-xxxhdpi-kalliope_brain.png" />
        <icon density="xhdpi" src="resources/android/kalliope_brain/drawable-xxxhdpi-kalliope_brain.png" />
        <icon density="xxhdpi" src="resources/android/kalliope_brain/drawable-xxxhdpi-kalliope_brain.png" />
        <icon density="xxxhdpi" src="resources/android/kalliope_brain/drawable-xxxhdpi-kalliope_brain.png" />
        <splash density="land-ldpi" src="resources/splash.png" />
        <splash density="land-mdpi" src="resources/splash.png" />
        <splash density="land-hdpi" src="resources/splash.png" />
        <splash density="land-xhdpi" src="resources/splash.png" />
        <splash density="land-xxhdpi" src="resources/splash.png" />
        <splash density="land-xxxhdpi" src="resources/splash.png" />
        <splash density="port-ldpi" src="resources/splash.png" />
        <splash density="port-mdpi" src="resources/splash.png" />
        <splash density="port-hdpi" src="resources/splash.png" />
        <splash density="port-xhdpi" src="resources/splash.png" />
        <splash density="port-xxhdpi" src="resources/splash.png" />
        <splash density="port-xxxhdpi" src="resources/splash.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/kalliope_brain/Default~iphone.png" width="57" />
        <icon height="114" src="resources/ios/kalliope_brain/Default~iphone.png" width="114" />
        <icon height="40" src="resources/ios/kalliope_brain/Default~iphone.png" width="40" />
        <icon height="80" src="resources/ios/kalliope_brain/Default~iphone.png" width="80" />
        <icon height="120" src="resources/ios/kalliope_brain/Default~iphone.png" width="120" />
        <icon height="50" src="resources/ios/kalliope_brain/Default~iphone.png" width="50" />
        <icon height="100" src="resources/ios/kalliope_brain/Default~iphone.png" width="100" />
        <icon height="60" src="resources/ios/kalliope_brain/Default~iphone.png" width="60" />
        <icon height="120" src="resources/ios/kalliope_brain/Default~iphone.png" width="120" />
        <icon height="180" src="resources/ios/kalliope_brain/Default~iphone.png" width="180" />
        <icon height="72" src="resources/ios/kalliope_brain/Default~iphone.png" width="72" />
        <icon height="144" src="resources/ios/kalliope_brain/Default~iphone.png" width="144" />
        <icon height="76" src="resources/ios/kalliope_brain/Default~iphone.png" width="76" />
        <icon height="152" src="resources/ios/kalliope_brain/Default~iphone.png" width="152" />
        <icon height="167" src="resources/ios/kalliope_brain/Default~iphone.png" width="167" />
        <icon height="29" src="resources/ios/kalliope_brain/Default~iphone.png" width="29" />
        <icon height="58" src="resources/ios/kalliope_brain/Default~iphone.png" width="58" />
        <icon height="87" src="resources/ios/kalliope_brain/Default~iphone.png" width="87" />
        <splash height="1136" src="resources/splash.png" width="640" />
        <splash height="1334" src="resources/splash.png" width="750" />
        <splash height="2208" src="resources/splash.png" width="1242" />
        <splash height="1242" src="resources/splash.png" width="2208" />
        <splash height="1536" src="resources/splash.png" width="2048" />
        <splash height="2048" src="resources/splash.png" width="2732" />
        <splash height="768" src="resources/splash.png" width="1024" />
        <splash height="2048" src="resources/splash.png" width="1536" />
        <splash height="2732" src="resources/splash.png" width="2048" />
        <splash height="1024" src="resources/splash.png" width="768" />
        <splash height="960" src="resources/splash.png" width="640" />
        <splash height="480" src="resources/splash.png" width="320" />
    </platform>
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <engine name="browser" spec="5.0.3" />
    <engine name="android" spec="~7.1.0" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="cordova-plugin-geofence" spec="^0.7.0" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <plugin name="cordova-plugin-advanced-http" spec="^2.0.1" />
</widget>
